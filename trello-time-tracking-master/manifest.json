{
  "name": "Trello TimeTracking",
  "version": "0.0.3",
  "manifest_version": 3,
  "description": "Small extension to log hours on a Trello card",
  "homepage_url": "http://www.google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://trello.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ],
      "run_at": "document_end"
    }
  ]
}
